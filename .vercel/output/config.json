{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/.*$",
      "dest": "/404.html",
      "status": 404
    }
  ],
  "images": {
    "sizes": [320, 640, 1280],
    "domains": ["images.unsplash.com", "hashnode.com", "cdn.hashnode.com"],
    "remotePatterns": []
  }
}
